{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "overrides": [
        {
            "files": ["./routes/**/*.js", "./database/**/*.js"],
            "rules": {
                "no-invalid-this": "off"
            }
        },
        {
            "files": ["./routes/**/*.js"],
            "rules": {
                "new-cap": "off"
            }
        }
    ],
    "rules": {
    }
}